<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="co.humaniq.views.GreeterActivity"
    android:background="@color/colorPrimary">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:background="@drawable/rounded_rect_white"
        android:paddingEnd="10dp" android:paddingLeft="10dp" android:paddingRight="10dp"
        android:paddingStart="10dp" android:paddingTop="1dp"
        android:layout_marginRight="16dp" app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView23" android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/imageView23" android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView23" android:layout_marginBottom="8dp"
        android:id="@+id/linearLayout8">
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView11"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
    </LinearLayout>
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:background="?attr/colorPrimary"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" tools:layout_editor_absoluteY="0dp">
        <TextView
            style="@style/ToolbarTitle"
            android:text="@string/app_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/toolbar_title" />
    </android.support.v7.widget.Toolbar>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="48dp" app:srcCompat="@mipmap/call_email"
        android:id="@+id/imageView20" android:padding="5dp" android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" android:layout_marginStart="16dp"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="28dp" app:srcCompat="@mipmap/parallels"
        android:id="@+id/imageView21" android:padding="5dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView22" android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView23"
        android:layout_marginStart="16dp" android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="56dp" app:srcCompat="@mipmap/lock"
        android:id="@+id/imageView23" android:padding="5dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonSignature"
        android:layout_marginStart="16dp" android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
    />
    <ImageView
        android:layout_width="80dp"
        android:layout_height="56dp" app:srcCompat="@mipmap/lock"
        android:id="@+id/imageView22" android:padding="5dp"
        android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/imageView18"
        android:layout_marginStart="16dp" android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout8"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:background="@drawable/rounded_rect_white" android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView22" android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22" android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/imageView22" android:layout_marginTop="8dp"
        android:paddingEnd="10dp" android:paddingLeft="10dp" android:paddingRight="10dp"
        android:paddingStart="10dp" android:paddingTop="1dp" android:id="@+id/linearLayout7">
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView11"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
        <TextView
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_weight="1" android:textColor="@color/lightBlue" android:textSize="42sp"
            android:gravity="center_vertical|center_horizontal" android:paddingTop="3dp"/>
    </LinearLayout>
    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="+7 (905) 605 0589"
        android:ems="10"
        android:id="@+id/editText4"
        android:background="@drawable/rounded_rect_white" android:layout_weight="1"
        android:textColor="@color/lightBlue" android:textSize="20sp" android:padding="10dp"
        android:layout_marginRight="16dp" app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView20"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/imageView20" android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView20"
        android:layout_marginBottom="8dp" android:layout_marginEnd="16dp"
        android:gravity="center_horizontal"/>
    <ImageButton
        android:layout_width="125dp"
        android:layout_height="80dp"
        app:srcCompat="@mipmap/arrow_right"
        android:id="@+id/buttonSignature"
        style="@style/RegularButton"
        android:paddingBottom="25dp" android:paddingTop="25dp" android:paddingLeft="10dp"
        android:paddingRight="10dp" android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
        android:layout_marginLeft="8dp" app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp" app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        android:layout_marginStart="6dp" android:layout_marginEnd="8dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="32dp" app:srcCompat="@mipmap/arrow_down"
        android:id="@+id/imageView18" android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintHorizontal_bias="0.504"
        android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/editText4"
        android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/linearLayout7"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="24dp" app:srcCompat="@mipmap/check"
        android:id="@+id/imageView19" android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8" android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/linearLayout8"/>

</android.support.constraint.ConstraintLayout>
