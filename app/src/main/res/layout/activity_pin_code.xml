<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="co.humaniq.views.GreeterActivity" android:background="@color/colorPrimary">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:background="?attr/colorPrimary"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <TextView
            android:id="@+id/toolbar_title"
            style="@style/ToolbarTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/app_name" />
        <ImageView android:layout_width="match_parent" android:layout_height="match_parent"
                   app:srcCompat="@mipmap/logo" android:paddingTop="15dp"
                   android:paddingBottom="15dp" android:id="@+id/toolbar_logo"/>
    </android.support.v7.widget.Toolbar>

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
        android:id="@+id/tableLayout" android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9">

        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/giraffe_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/elephant_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/koala_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
        </TableRow>
        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/lion_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/kangaroo_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/tiget_layout"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
        </TableRow>
        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/raccoon_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/hedgehog_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/lemur_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
        </TableRow>
        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/chameleon_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <include android:layout_width="@dimen/pin_key_size"
                     android:layout_height="@dimen/pin_key_size"
                     layout="@layout/button_pin_key"
                     app:layout_constraintLeft_toLeftOf="parent"
                     app:layout_constraintRight_toLeftOf="@+id/guideline1"
                     app:layout_constraintHorizontal_bias="1.0"
                     app:layout_constraintDimensionRatio="h,1:1"
                     android:id="@+id/cat_include"
                     app:layout_constraintTop_toBottomOf="@+id/imageView36"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintVertical_bias="1.0"/>
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent" app:srcCompat="@drawable/ic_next_blue"
                android:id="@+id/nextStepButton" android:layout_margin="10dp"
                android:background="@drawable/selector_circle_white_button"/>
        </TableRow>
    </TableLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp" app:layout_constraintLeft_toLeftOf="@+id/tableLayout"
        android:layout_marginRight="0dp" app:layout_constraintRight_toRightOf="@+id/tableLayout"
        android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintHorizontal_bias="0.0" android:id="@+id/linearLayout9">
        <include
            layout="@layout/button_pin_key"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline1"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintDimensionRatio="h,1:1"
            android:id="@+id/pin_place_1"
            app:layout_constraintTop_toBottomOf="@+id/imageView36"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0" android:layout_weight="1"
            android:layout_width="0dp" android:layout_height="72dp"/>
        <include
            layout="@layout/button_pin_key"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline1"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintDimensionRatio="h,1:1"
            android:id="@+id/pin_place_2"
            app:layout_constraintTop_toBottomOf="@+id/imageView36"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0" android:layout_weight="1"
            android:layout_width="0dp" android:layout_height="72dp"/>
        <include
            layout="@layout/button_pin_key"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline1"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintDimensionRatio="h,1:1"
            android:id="@+id/pin_place_3"
            app:layout_constraintTop_toBottomOf="@+id/imageView36"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0" android:layout_weight="1"
            android:layout_width="0dp" android:layout_height="72dp"/>
        <include
            layout="@layout/button_pin_key"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline1"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintDimensionRatio="h,1:1"
            android:id="@+id/pin_place_4"
            app:layout_constraintTop_toBottomOf="@+id/imageView36"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0" android:layout_weight="1"
            android:layout_width="0dp" android:layout_height="72dp"/>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
